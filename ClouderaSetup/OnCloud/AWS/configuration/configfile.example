# Example configfile (For Reference): Update the values accordingly in original configfile
PROVISION_KEYCLOAK:YES
KEYCLOAK_ADMIN_PASSWORD:xxxxxx
AWS_REGION:ap-southeast-1
CDP_DEPLOYMENT_TYPE:public
WORKSHOP_NAME:pse-cdpdemo
NUMBER_OF_WORKSHOP_USERS:2
WORKSHOP_USER_PREFIX:pse-cdpdemo-user
WORKSHOP_USER_DEFAULT_PASSWORD:changeme
LOCAL_MACHINE_IP:172.16.31.xx/32
ENABLE_DATA_SERVICES:[CML,CDW,CDE]

